{
  "name": "@plebbit/plebbit-react-hooks",
  "keywords": [
    "plebbit",
    "subplebbit"
  ],
  "repository": "github:plebbit/plebbit-react-hooks",
  "license": "GPL-2.0",
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "version": "0.0.1",
  "files": [
    "dist"
  ],
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "prebuild": "rm -rf dist",
    "build": "tsc --project config/tsconfig.json",
    "build:watch": "tsc --project config/tsconfig.json --watch",
    "webpack": "webpack --mode development --config config/webpack.config.cjs",
    "webpack:watch": "while true; do webpack watch --mode development --config config/webpack.config.cjs; sleep 1; done",
    "test": "jest --config config/jest.config.ts",
    "test:e2e": "yarn test:e2e:mock && yarn test:e2e:browser && yarn test:e2e:electron",
    "test:e2e:mock": "karma start config/karma.conf.cjs --plebbit-js-mock",
    "test:e2e:mock-content": "karma start config/karma.conf.cjs --plebbit-js-mock-content",
    "test:e2e:browser": "karma start config/karma.conf.cjs",
    "test:e2e:electron": "rm -rf /tmp/.plebbit-electron && karma start config/karma-electron.conf.cjs",
    "test:server": "rm -rf .plebbit/ && node test/test-server",
    "test:server:wait-on": "wait-on http://localhost:59281 --timeout 120000",
    "test:mock-content": "jest --config config/jest.config.ts --testRegex plebbit-js-mock-content.donttest.ts",
    "prettier": "prettier {src,test,config}/**/*.{cjs,js,jsx,ts,tsx} --config config/prettier.config.js --write",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0"
  },
  "dependencies": {
    "@plebbit/plebbit-js": "../plebbit-js",
    "@plebbit/plebbit-logger": "https://github.com/plebbit/plebbit-logger.git",
    "assert": "2.0.0",
    "ethers": "5.6.9",
    "localforage": "1.10.0",
    "lodash.isequal": "4.5.0",
    "memoizee": "0.4.15",
    "quick-lru": "5.1.1",
    "uint8arrays": "3.0.0",
    "uuid": "9.0.1",
    "zustand": "4.0.0"
  },
  "peerDependencies": {
    "react": ">=16.8"
  },
  "devDependencies": {
    "@babel/plugin-transform-runtime": "7.17.12",
    "@babel/preset-env": "7.17.12",
    "@financial-times/jest-browser-resolver": "^1.0.2",
    "@testing-library/react-hooks": "7.0.2",
    "@tsconfig/recommended": "1.0.3",
    "@types/jest": "29.5.12",
    "@types/lodash.isequal": "4.5.6",
    "@types/memoizee": "0.4.8",
    "@types/node": "18.19.0",
    "@types/react": "17.0.21",
    "@types/uuid": "8.3.2",
    "@typescript-eslint/eslint-plugin": "5.7.0",
    "@typescript-eslint/parser": "5.7.0",
    "babel-loader": "9.1.3",
    "browser-resolve": "2.0.0",
    "chai": "5.0.3",
    "chokidar": "3.5.3",
    "conventional-changelog-cli": "4.1.0",
    "cz-conventional-changelog": "3.3.0",
    "dotenv": "16.0.1",
    "electron": "19.0.1",
    "enhanced-resolve-jest": "1.1.0",
    "eslint": "8.4.1",
    "eslint-plugin-react-hooks": "4.3.0",
    "eslint-plugin-tsdoc": "0.2.14",
    "fs-extra": "10.1.0",
    "go-ipfs": "0.16.0",
    "husky": "4.3.8",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "karma": "6.4.2",
    "karma-chai": "0.1.0",
    "karma-chrome-launcher": "3.2.0",
    "karma-electron": "7.3.0",
    "karma-firefox-launcher": "2.1.2",
    "karma-mocha": "2.0.1",
    "karma-sinon": "1.0.5",
    "karma-spec-reporter": "0.0.36",
    "lint-staged": "12.3.8",
    "mocha": "10.2.0",
    "prettier": "3.2.5",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-test-renderer": "17.0.2",
    "sinon": "14.0.0",
    "tcp-port-used": "1.0.2",
    "tempy": "1.0.1",
    "ts-jest": "29.1.2",
    "ts-node": "10.9.2",
    "typescript": "5.3.3",
    "wait-on": "6.0.1",
    "webpack": "5.90.1",
    "webpack-cli": "5.1.4",
    "webpack-watch-files-plugin": "1.2.1"
  },
  "lint-staged": {
    "{src,test,config}/**/*.{cjs,js,jsx,ts,tsx}": [
      "prettier --config config/prettier.config.js --write"
    ]
  },
  "husky": {
    "hooks": {
      "prepare-commit-msg": "exec < /dev/tty && cz --hook || true",
      "pre-commit": "lint-staged"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
